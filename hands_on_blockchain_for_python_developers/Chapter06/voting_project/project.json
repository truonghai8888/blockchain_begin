{
  "chains": {
    "mainnet": {
      "chain": {
        "class": "populus.chain.geth.MainnetChain"
      },
      "contracts": {
        "backends": {
          "JSONFile": {
            "class": "populus.contracts.backends.filesystem.JSONFileBackend",
            "priority": 10,
            "settings": {
              "file_path": "./registrar.json"
            }
          },
          "Memory": {
            "class": "populus.contracts.backends.memory.MemoryBackend",
            "priority": 50
          },
          "ProjectContracts": {
            "class": "populus.contracts.backends.project.ProjectContractsBackend",
            "priority": 20
          },
          "TestContracts": {
            "class": "populus.contracts.backends.testing.TestContractsBackend",
            "priority": 40
          }
        }
      },
      "web3": {
        "provider": {
          "class": "web3.providers.ipc.IPCProvider"
        }
      }
    },
    "ropsten": {
      "chain": {
        "class": "populus.chain.geth.TestnetChain"
      },
      "contracts": {
        "backends": {
          "JSONFile": {
            "class": "populus.contracts.backends.filesystem.JSONFileBackend",
            "priority": 10,
            "settings": {
              "file_path": "./registrar.json"
            }
          },
          "Memory": {
            "class": "populus.contracts.backends.memory.MemoryBackend",
            "priority": 50
          },
          "ProjectContracts": {
            "class": "populus.contracts.backends.project.ProjectContractsBackend",
            "priority": 20
          },
          "TestContracts": {
            "class": "populus.contracts.backends.testing.TestContractsBackend",
            "priority": 40
          }
        }
      },
      "web3": {
        "provider": {
          "class": "web3.providers.ipc.IPCProvider"
        }
      }
    },
    "temp": {
      "chain": {
        "class": "populus.chain.geth.TemporaryGethChain"
      },
      "contracts": {
        "backends": {
          "Memory": {
            "class": "populus.contracts.backends.memory.MemoryBackend",
            "priority": 50
          },
          "ProjectContracts": {
            "class": "populus.contracts.backends.project.ProjectContractsBackend",
            "priority": 20
          },
          "TestContracts": {
            "class": "populus.contracts.backends.testing.TestContractsBackend",
            "priority": 40
          }
        }
      },
      "web3": {
        "provider": {
          "class": "web3.providers.ipc.IPCProvider"
        }
      }
    },
    "tester": {
      "chain": {
        "class": "populus.chain.tester.TesterChain"
      },
      "contracts": {
        "backends": {
          "Memory": {
            "class": "populus.contracts.backends.memory.MemoryBackend",
            "priority": 50
          },
          "ProjectContracts": {
            "class": "populus.contracts.backends.project.ProjectContractsBackend",
            "priority": 20
          },
          "TestContracts": {
            "class": "populus.contracts.backends.testing.TestContractsBackend",
            "priority": 40
          }
        }
      },
      "web3": {
        "provider": {
          "class": "web3.providers.eth_tester.EthereumTesterProvider"
        }
      }
    },
    "ganache": {
      "chain": {
        "class": "populus.chain.ExternalChain"
      },
      "web3": {
        "provider": {
          "class": "web3.providers.HTTPProvider",
          "settings": {
            "endpoint_uri": "http://localhost:8545"
          }
        }
      },
      "contracts": {
        "backends": {
          "JSONFile": {"$ref": "contracts.backends.JSONFile"},
          "ProjectContracts": {
            "$ref": "contracts.backends.ProjectContracts"
          }
        }
      }
    },
    "localblock": {
      "chain": {
        "class": "populus.chain.ExternalChain"
      },
      "web3": {
        "provider": {
          "class": "web3.providers.ipc.IPCProvider",
        "settings": {
          "ipc_path":"/tmp/geth.ipc"
        }
       }
      },
      "contracts": {
        "backends": {
          "JSONFile": {"$ref": "contracts.backends.JSONFile"},
          "ProjectContracts": {
            "$ref": "contracts.backends.ProjectContracts"
          }
        }
      }
    }
  },
  "compilation": {
    "backend": {
      "class": "populus.compilation.backends.VyperBackend"
    },
    "contract_source_dirs": [
      "./contracts"
    ],
    "import_remappings": []
  },
  "contracts": {
    "backends": {
      "JSONFile": {
        "class": "populus.contracts.backends.filesystem.JSONFileBackend",
        "priority": 10,
        "settings": {
          "file_path": "./registrar.json"
        }
      },
      "Memory": {
        "class": "populus.contracts.backends.memory.MemoryBackend",
        "priority": 50
      },
      "ProjectContracts": {
        "class": "populus.contracts.backends.project.ProjectContractsBackend",
        "priority": 20
      },
      "TestContracts": {
        "class": "populus.contracts.backends.testing.TestContractsBackend",
        "priority": 40
      }
    }
  },
  "version": "9",
  "web3": {
    "GethIPC": {
      "provider": {
        "class": "web3.providers.ipc.IPCProvider"
      }
    },
    "InfuraMainnet": {
      "eth": {
        "default_account": "0x0000000000000000000000000000000000000001"
      },
      "provider": {
        "class": "web3.providers.HTTPProvider",
        "settings": {
          "endpoint_uri": "https://mainnet.infura.io"
        }
      }
    },
    "InfuraRopsten": {
      "eth": {
        "default_account": "0x0000000000000000000000000000000000000001"
      },
      "provider": {
        "class": "web3.providers.HTTPProvider",
        "settings": {
          "endpoint_uri": "https://ropsten.infura.io"
        }
      }
    },
    "Tester": {
      "provider": {
        "class": "web3.providers.eth_tester.EthereumTesterProvider"
      }
    }
  }
}
